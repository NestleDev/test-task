<ul class="users__list">
    {{#each items}}
    <li class="users__item">
        <div class="user">
            <div class="user__photo">
                <img src="{{photo}}" alt="{{name}}" class="user__img">
            </div>
            <div class="user__name">{{name}}</div>
        </div>
    </li>
    {{/each}}
</ul>